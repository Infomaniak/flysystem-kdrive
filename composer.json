{
    "name": "infomaniak/flysystem-kdrive",
    "description": "Flysystem Adapter for Infomaniak kDrive",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolas Hedger",
            "email": "nicolas.hedger@infomaniak.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Infomaniak\\KDrive\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "ext-curl": "*",
        "league/flysystem-webdav": "^1.0"
